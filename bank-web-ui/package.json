{
  "name": "enki-bank-web-ui",
  "version": "0.1.5",
  "description": "ENKI Bank web UI",
  "repository": {
    "type": "git",
    "url": "git@github.com:lshift/enki-all.git"
  },
  "private": true,
  "main": "server.js",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "test": "yarn run eslint && yarn run allTestsWithCoverage",
    "eslint": "eslint .",
    "start": "nodemon server.js --exec babel-node",
    "start:client": "webpack-dev-server --hot --inline --host 0.0.0.0",
    "start:server": "cross-env ENABLE_WEBPACK_DEV_SERVER=true nodemon --inspect server.js --exec babel-node",
    "start:client-win": "webpack-dev-server --lazy --host 0.0.0.0",
    "start:server-win": "cross-env ENABLE_WEBPACK_DEV_SERVER=true nodemon --legacy-watch server.js --exec babel-node",
    "testPath": "JUNIT_REPORT_PATH=./unitTests.xml mocha --require @babel/register --colors --reporter spec",
    "debugPath": "node --inspect --inspect-brk mocha --require @babel/register -R spec --colors",
    "unitTest": "JUNIT_REPORT_PATH=./unitTests.xml mocha --require @babel/register --colors --reporter spec test/unit",
    "integrationTest": "JUNIT_REPORT_PATH=./integrationTests.xml cross-env NODE_ENV=test mocha --require @babel/register test/integration --reporter spec --exit",
    "allTestsWithCoverage": "JUNIT_REPORT_PATH=./allTests.xml cross-env NODE_ENV=test nyc mocha --exit --reporter mocha-jenkins-reporter test && nyc report --reporter=cobertura",
    "webpack": "webpack",
    "webpack-watch": "webpack --progress --colors --watch",
    "preversion": "yarn test",
    "postversion": "git reset --soft HEAD~1 && git commit --amend -m \"$(git log --oneline --format=%B -n 1)\"",
    "db:clean": "babel-node server/tools/with-config-do.js db:clean"
  },
  "pre-commit": [
    "eslint"
  ],
  "keywords": [
    "vue",
    "express"
  ],
  "author": {
    "name": "Oliver Wyman Labs",
    "email": "enki-questions@oliverwyman.com",
    "url": "http://labs.oliverwyman.com/"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "bluebird": "^3.5.4",
    "body-parser": "^1.19.0",
    "bootstrap": "^3.3.7",
    "busboy": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "css-loader": "^2.1.1",
    "csurf": "^1.10.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.16.1",
    "file-loader": "^3.0.1",
    "handlebars": "^4.1.2",
    "helmet": "^3.18.0",
    "hydra-js": "^0.1.5",
    "jquery": "^3.5.0",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.19.0",
    "npm-font-open-sans": "^1.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "request-promise-native": "^1.0.7",
    "sqlite3": "^4.0.8",
    "terser-webpack-plugin": "^1.2.4",
    "typeorm": "^0.2.17",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-flag-icon": "^1.0.6",
    "vue-form-generator": "^2.3.4",
    "vue-form-wizard": "^0.8.4",
    "vue-js-modal": "^1.3.31",
    "vue-loader": "^15.7.0",
    "vue-password": "^1.2.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.1.1",
    "webpack": "^4.31.0",
    "webpack-manifest-plugin": "^2.0.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "cross-env": "^5.2.0",
    "deep-diff": "^1.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "less": "^3.9.0",
    "memorystream": "^0.3.1",
    "mocha": "^6.1.4",
    "mocha-jenkins-reporter": "^0.4.1",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "engines": {
    "node": ">= 6.9.5"
  }
}
